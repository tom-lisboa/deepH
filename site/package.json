{
  "name": "deeph-site",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "clean": "node -e \"const fs=require('fs'); for (const p of ['.next','.next-dev','tsconfig.tsbuildinfo']) fs.rmSync(p,{ recursive:true, force:true });\"",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "next": "15.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/node": "22.13.5",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "typescript": "5.8.2"
  }
}
